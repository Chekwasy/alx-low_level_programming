#include "main.h"
#include <stdio.h>
#include <stdlib.h>
/**
 * add - add two numbers
 * @a: add para 1
 * @b: add para 2
 * return: sum
 */
int add(int a, int b)
{
	return (a+b);
}
/**
 * sub - sub two numbers
 * @a: para 1
 * @b: para 2
 * return: sub
 */
int sub(int a, int b)
{
	return (a-b);
}
/**
 * mul - mul two numbers
 * @a: para 1
 * @b: para 2
 * return: mul
 */
int mul(int a, int b)
{
	return (a*b);
}
/**
 * Div - div two numbers
 * @a: para 1
 * @b: para 2
 * return: div
 */
int Div(int a, int b)
{
	return (a/b);
}
/**
 * mod - mod two numbers
 * @a: para 1
 * @b: para 2
 * return: mod
 */
int mod(int a, int b)
{
	return (a%b);
}
